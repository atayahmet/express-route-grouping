{
  "version": "1.1.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --coverage",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "release": "standard-version"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "express-route-grouping",
  "description": "Easy route grouping for Express JS with resource api model.",
  "author": "Ahmet ATAY",
  "keywords": [
    "express",
    "route",
    "group",
    "end-point",
    "resource",
    "api"
  ],
  "module": "dist/express-route-grouping.esm.js",
  "size-limit": [
    {
      "path": "dist/express-route-grouping.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/express-route-grouping.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@size-limit/preset-small-lib": "^4.10.0",
    "@types/express": "^4.17.11",
    "@types/pluralize": "^0.0.29",
    "coveralls": "^3.1.0",
    "husky": "^4.3.8",
    "size-limit": "^4.10.0",
    "standard-version": "^9.1.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "express": "^4.17.1",
    "pluralize": "^8.0.0"
  }
}
